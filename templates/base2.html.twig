<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="keywords" content="{% block meta_keywords %}{% endblock %}">
    <meta name="description" content="{% block meta_description %}{% endblock %}">
    <meta name="author" content="{% block meta_author %}{% endblock %}">
    <link rel="shortcut icon" href="{{ asset('images/fevicon.png') }}" type="image/png">

    <title>{% block title %}Tripin{% endblock %}</title>

    {# Bootstrap core CSS #}
    <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">
    {# Fonts Style #}
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700;900&display=swap" rel="stylesheet">
    {# Owl Carousel #}
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css">
    {# Font Awesome #}
    <link rel="stylesheet" href="{{ asset('css/font-awesome.min.css') }}">
    {# Custom Styles #}
    <link rel="stylesheet" href="{{ asset('css/style.css') }}">
    <link rel="stylesheet" href="{{ asset('css/responsive.css') }}">

    {% block stylesheets %}{% endblock %}
</head>
<body>

    {# Header Section #}
    <header class="header_section">
        <div class="header_top">
            <div class="container-fluid">
                <div class="contact_nav">
                    <a href="#"><i class="fa fa-phone"></i><span> Call : +01 123455678990</span></a>
                    <a href="#"><i class="fa fa-envelope"></i><span> Email : demo@gmail.com</span></a>
                    <a href="#"><i class="fa fa-map-marker"></i><span> Location</span></a>
                </div>
            </div>
        </div>
        <div class="header_bottom">
            <div class="container-fluid">
                <nav class="navbar navbar-expand-lg custom_nav-container">
                    <a class="navbar-brand" href="{{ path('landing_index') }}"><span>Tripin</span></a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav">
                            <li class="nav-item {% block nav_home_active %}{% endblock %}">
                                <a class="nav-link" href="{{ path('landing_index') }}">Home</a>
                            </li>
                            <li class="nav-item {% block nav_services_active %}{% endblock %}">
                                <a class="nav-link" href="{{ path('landing_service') }}">Services</a>
                            </li>
                            <li class="nav-item {% block nav_about_active %}{% endblock %}">
                                <a class="nav-link" href="{{ path('landing_about') }}">About</a>
                            </li>
                            <li class="nav-item {% block nav_activity_active %}{% endblock %}">
                                <a class="nav-link" href="{{ path('app_destination_front_index') }}">Destination</a>
                            </li>
                             <li class="nav-item">
                                             <a class="nav-link" href="{{ path('landing_service2') }}">offres</a>
                                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#"><i class="fa fa-user"></i> Login</a>
                            </li>
                        </ul>
                        <form class="form-inline ml-auto">
                            <button class="btn my-2 my-sm-0 nav_search-btn" type="submit"><i class="fa fa-search"></i></button>
                        </form>
                    </div>
                </nav>
            </div>
        </div>
    </header>

    {# Main Content #}
    <main>
        {% block content %}{% endblock %}
    </main>

    {# Footer Section #}
    <footer class="footer_section">
        <div class="container">
            <p>&copy; <span id="displayYear"></span> All Rights Reserved By <a href="https://html.design/">Free Html Templates</a></p>
        </div>
    </footer>

    {# jQuery #}
    <script src="{{ asset('js/jquery-3.4.1.min.js') }}"></script>
    {# Popper JS #}
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script>
    {# Bootstrap JS #}
    <script src="{{ asset('js/bootstrap.js') }}"></script>
    {# Owl Carousel #}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
    {# Custom JS #}
    <script src="{{ asset('js/custom.js') }}"></script>
    {# Google Maps API #}
    <script src="https://maps.googleapis.com/maps/api/js?key=YOUR_API_KEY&callback=myMap"></script>

    {% block javascripts %}{% endblock %}

    <script>
        // Display current year
        document.getElementById('displayYear').textContent = new Date().getFullYear();
    </script>
</body>
</html>